							{
								"activity": "gm_OnPremFile",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@activity('gm_OnPremFile').output.childItems",
								"type": "Expression"
