				"typeProperties": {
					"domain": "https://adb-7405607101562799.19.azuredatabricks.net",
					"clusterId": "0131-080551-qcvugeqq",
					"accessToken": {
						"type": "SecureString",
						"value": "[parameters('ls_deltalake_accessToken')]"
