					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('ls_emr_sql_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('ls_emr_sql_password')]"
